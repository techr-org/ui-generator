{
  "name": "@techrdev/ui-generator",
  "version": "0.0.3",
  "description": "Techr UI Generator for Design Tokens and more",
  "keywords": [
    "ui-generator",
    "ui",
    "generator",
    "techr",
    "design",
    "tokens",
    "theme",
    "css",
    "postcss"
  ],
  "license": "MIT",
  "repository": "techrdev/ui-generator",
  "author": {
    "name": "Alessio Occhipinti",
    "email": "info@godev.space",
    "url": "https://godev.space"
  },
  "scripts": {
    "dev": "ts-node src/cli",
    "build": "del-cli dist && tsc",
    "prepack": "yarn build",
    "lint": "eslint . --ext ts",
    "test": "tsc --noEmit && jest --coverage",
    "release": "git pull && standard-version && git push --follow-tags"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "publishConfig": {
    "access": "public",
    "@techrdev:registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "cssbeautify": "0.3.1",
    "figlet": "1.4.0",
    "fs-extra": "9.0.0",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@lasalefamine/eslint-config-base": "0.2.0",
    "@lasalefamine/tsconfig": "0.1.0",
    "@types/cssbeautify": "0.3.1",
    "@types/figlet": "1.2.0",
    "@types/fs-extra": "9.0.1",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.6",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "del-cli": "3.0.1",
    "eslint": "7.1.0",
    "eslint-plugin-import": "2.20.2",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "jest-file-snapshot": "0.3.8",
    "lint-staged": "10.2.7",
    "rimraf": "3.0.2",
    "standard-version": "8.0.0",
    "ts-jest": "26.0.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.3"
  },
  "files": [
    "dist/src",
    "!*.test.*"
  ],
  "main": "dist",
  "types": "dist",
  "bin": {
    "techrui": "dist/cli.js"
  }
}
